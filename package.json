{
    "name": "angular-three",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "postinstall": "ngcc --properties es2015 browser module main",
        "commit": "git-cz",
        "contributors:init": "all-contributors init",
        "contributors:add": "all-contributors add",
        "release": "dotenv release-it --",
        "storybook": "nx storybook soba",
        "build": "nx run-many --target=build --projects=core,cannon,postprocessing,soba,schematics",
        "publish": "nx run-many --target=publish --all --parallel",
        "predocs": "test \"$CI\" = true && npx pnpm install --no-frozen-lockfile -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
        "docs": "npx pnpm run predocs && nx build-docs documentations"
    },
    "private": true,
    "dependencies": {
        "@storybook/core-server": "6.4.22",
        "tslib": "^2.3.1"
    },
